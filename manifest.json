  "manifest_version": 3,
  "version": "1.0.0",
  "name": "osu! Rank Trend Line",
  "description": "Shows a rank trend line and projected rank on osu! user profiles.",
  "icons": {
    "128": "extension-icon.png",
    "256": "extension-icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://osu.ppy.sh/*"],
      "js": ["content.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{b620affb-29e0-44c7-a491-290e3118fead}"
    }
  }
}
